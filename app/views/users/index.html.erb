<%= render 'books/list', books:@book %>
<div class= 'col-md-8 offset-md-1'>
<h2>Users</h2>
<table class='table'>
<thead>
<tr>
<th>image</th>
<th>name</th>
<th colspan="3"></th>
</tr>
</thead>
<tbody>
<tr>
<% @user.each do |user| %>
<td>
<%= image_tag current_user.get_profile_image(60,60) %>
</td>
<td><%= user.name %></td>
<td><%= link_to "Show",user %></td>
<% end %>
</tr>
</tbody>
</table>
</div>


