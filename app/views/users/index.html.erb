<h1>ここはindexページです。</h1>

<h4>画像</h4>
<h1>User info</h1>
<%= image_tag current_user.get_profile_image(100,100)%>

<h1>New Book</h1>

<%= form_with model:@book,url: '/books',method: :post do |f| %>
<h4>Title</h4>
<%= f.text_field :title %>

<h4>Opinion</h4>
<%= f.text_area :body %>

<p><%= f.submit'Create Book' %></p>
<% end %>